use io 
use conv

main(args:int[][]) {
  a:int[][] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}}
  b:int[][] = {{1, 2, 3}, {4, 5, 6}, {7, 8, 9}}
  c:int[][] = {{0, 0, 0}, {0, 0, 0}, {0, 0, 0}}
  i:int = 0 
  j:int = 0 
  while(i < length(a)) {
    while(j < length(a[0])) {
      c[i][j] = a[i][j] + b[i][j]
      j = j + 1
    }
    i = i + 1
    j = 0
  }

  println(unparseInt(c[0][0]))
}